{
  "name": "dutymate-mobile",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "expo start",
    "android:dev": "APP_ENV=development expo start --android",
    "android:prod": "APP_ENV=production expo start --android",
    "ios:dev": "APP_ENV=development expo start --ios",
    "ios:prod": "APP_ENV=production expo start --ios",
    "web:dev": "APP_ENV=development expo start --web",
    "web:prod": "APP_ENV=production expo start --web",
    "lint": "biome lint ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/stack": "^7.3.2",
    "expo": "~52.0.46",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-status-bar": "~2.0.1",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-webview": "13.12.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "1.9.4",
    "@types/react": "~18.3.12",
    "react-native-svg-transformer": "^1.5.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
